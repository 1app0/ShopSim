                        <Font name="System Bold" size="14.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="50.0" top="25.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
            <TableView fx:id="list" prefHeight="237.0" prefWidth="414.0">
              <columns>
                <TableColumn fx:id="idColumn" prefWidth="75.0" text="ID" />
                <TableColumn fx:id="nameColumn" prefWidth="155.0" text="NAME" />
                  <TableColumn fx:id="barcodeColumn" prefWidth="183.0" text="BARCODE" />
              </columns>
            </TableView>
            <Label fx:id="resultLabel" prefHeight="17.0" prefWidth="409.0" text="Label" />
         </children>
      </VBox>
   </children>
